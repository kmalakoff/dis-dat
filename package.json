{
  "name": "dis-dat",
  "version": "0.1.7",
  "description": "Run commands in parallel (dad / dis-and-dat) or sequentially (dtd / dis-then-dat)",
  "keywords": [
    "this",
    "dis",
    "then",
    "and",
    "that",
    "dat",
    "cli",
    "spawn",
    "exec",
    "execute",
    "series",
    "parallel",
    "run",
    "all",
    "run-all",
    "run-p",
    "run-s"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:kmalakoff/dis-dat.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "lib",
    "bin"
  ],
  "bin": {
    "dad": "./bin/dis-and-dat.js",
    "dtd": "./bin/dis-then-dat.js"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublishOnly": "./bin/dis-then-dat.js \"npm run lint\" \"depcheck\"",
    "test": "mocha-compat test/spec/**/*.test.js  --no-timeouts"
  },
  "dependencies": {
    "cross-spawn-cb": "^0.5.9",
    "exit": "^0.1.2",
    "getopts-compat": "^2.2.5",
    "just-extend": "^6.0.1",
    "queue-cb": "^1.1.6",
    "string-argv": "^0.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.30.0",
    "cr": "^0.1.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.3",
    "eslint-plugin-promise": "^6.0.0",
    "is-version": "^0.2.0",
    "mocha-compat": "^3.5.5",
    "prettier": "^2.7.1"
  },
  "engines": {
    "node": ">=0.8"
  }
}
